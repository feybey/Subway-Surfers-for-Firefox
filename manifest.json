{
  "manifest_version": 2,
  "name": "subwaySurfersFirefox",
  "version": "1.0.11",

  "description": "Adds subway surfers.",

  "icons": {
    "48": "icons/surfingIcon.png"
  },

  "browser_action": {
    "default_icon": "icons/surfingIcon.png"
  },

  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["activeTab", "tabs", "http://*/*", "https://*/*"]
}
